{
    "name": "Angular/NRWL Template Development Environment",
    "dockerComposeFile": "docker-compose.yml",
    "service": "node-practice-development-environment",
    "workspaceFolder": "/workspace",
    "settings": {
        "eslint.options": {
            "jshint.enable": false
        },
        "git.ignoreLimitWarning": true,
        "terminal.integrated.defaultProfile.linux": "/bin/bash",
        "stylelint": {
            "packageManager": "yarn",
            "configBasedir": ".stylelintrc.json",
            "configFile": ".stylelintrc.json"
        },
        "sortJSON.contextMenu": {
            "sortJSON": true,
            "sortJSONAlphaNum": true,
            "sortJSONAlphaNumReverse": false,
            "sortJSONKeyLength": false,
            "sortJSONKeyLengthReverse": false,
            "sortJSONReverse": false,
            "sortJSONType": false,
            "sortJSONTypeReverse": false,
            "sortJSONValues": false,
            "sortJSONValuesReverse": false
        }
    },
    "extensions": [
